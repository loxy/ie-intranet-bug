{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "noEmit": false,
    "noEmitHelpers": true,
    "importHelpers": true,
    "strictNullChecks": false,
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "/",
    "paths": {
      "*": [
        "node_modules/@types/*",
        "*"
      ]
    }
  },
  "compileOnSave": false,
  "buildOnSave": false
}
